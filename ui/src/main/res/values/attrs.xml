<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SlideMenu">
        <attr name="labels" format="string" />
        <attr name="selectedColor" format="color" />
        <attr name="unSelectedColor" format="color" />
        <attr name="labelTextSize" format="dimension" />
    </declare-styleable>
    <declare-styleable name="CheckBox">
        <attr name="uncheckedBackgroundColor" format="color"/>
        <attr name="checkedBackgroundColor" format="color" />
        <attr name="checkedColor" format="color" />
        <attr name="checked" format="boolean" />
        <attr name="checkbox_style">
            <enum name="round_rect_stroke" value="0" />
            <enum name="round_rect_fill" value="1" />
            <enum name="round_fill" value="2" />
            <enum name="round_stroke" value="3" />
        </attr>
    </declare-styleable>
    <declare-styleable name="SwitchView">
        <attr name="borderColor" format="color"/>
        <attr name="offColor" format="color" />
        <attr name="onColor" format="color" />
        <attr name="on" format="boolean" />
    </declare-styleable>
    <declare-styleable name="FallView">
        <attr name="fallview_column" format="integer"/>
    </declare-styleable>
</resources>
